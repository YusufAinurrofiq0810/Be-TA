    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "tmp": "^0.2.3",
    "uuid": "^10.0.0",
    "xendit-node": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@prisma/client": "^5.16.1",
    "@swc/cli": "^0.3.14",
    "@swc/core": "^1.6.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
